#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int n;
   scanf("%d",&n);
    int ans=1;
    while(n){
        if(n%2==1)
            {
            ans=ans*2;
           
        }
         n=n/2;
    }
    //cout<<ans<<endl;
    printf("%d",ans);
    return 0;
}
