#include<iostream>
#include<climits>
#include<string>
#include<algorithm>
#include<map>
#include<vector>
#include<set>
#define N 10009
#define MOD 1000000007
using namespace std;
typedef long long int ll;
int main()
{
	int n, m, b, c;
	cin >> n >> m >> b >> c;
	for (int i = 0; i < n; i++)
	{
		for (int j = 0; j < n; j++)
		{
			ll p;
			cin >> p;
		}
	}
	for (int j = 0; j < m; j++)
	{
		vector<int>vec;
		for (int i = 0; i < b; i++)
		{
			int x, y;
			cin >> x >> y;
			vec.push_back(x);
		}
		for (int i = 0; i < b; i++)
		{
			cout << vec[i] << endl;
		}
	}
}