#define intx(i,j,k,l) ((a[i]*b[j]-b[i]*a[j])*(a[k]-a[l])-(a[i]-a[j])*(a[k]*b[l]-b[k]*a[l]))/((a[i]-a[j])*(b[k]-b[l])-(b[i]-b[j])*(a[k]-a[l]))
#define inty(i,j,k,l) ((a[i]*b[j]-b[i]*a[j])*(b[k]-b[l])-(b[i]-b[j])*(a[k]*b[l]-b[k]*a[l]))/((a[i]-a[j])*(b[k]-b[l])-(b[i]-b[j])*(a[k]-a[l]))
#define dst(u,v,x,y) sqrt((x*1.0-u*1.0)*(x*1.0-u*1.0)+(y*1.0-v*1.0)*(y*1.0-v*1.0))
#define area(p1,p2,p3) (p1.xx*p2.yy+p2.xx*p3.yy+p3.xx*p1.yy-p1.yy*p2.xx-p2.yy*p3.xx-p3.yy*p1.xx)

int ts,kk=1;

#define M 1000005
#define MD 1000000007LL
#define MX 1001

int m;
int n,sz[30],it[30];
vector< int >v[30];
vector< int >b;

int a[M];

bool rec(int i)
{
    if(i==-1)return 1;

    for(int j=0;j<n;j++)
    {
        if(it[j]<sz[j]&& v[j][ it[j] ]==b[i] )
        {
            it[j]++;
            if(rec(i-1))return 1;
            it[j]--;
        }
    }
    return 0;
}

int main()
{
    int t,i,j,k;

    sc(ts);
    while(ts--)
    {
        m=0;
        sc(n);

        if(n==1)
        {
            bool g=1;
            sc(t);
            f(i,t)sc(a[i]);
            f(i,t)
            {
                sc(k);
                if(g&&a[t-i-1]!=k)
                    g=0;
            }
            if(g)printf("Yes\n");
            else printf("No\n");
            continue;
        }

        f(i,n)
        {
            sc(sz[i]);
            m+=sz[i];
            it[i]=0;
            v[i].clear();
            f(j,sz[i])sc(k),v[i].pb(k);
        }
        bool g=1;
        b.clear();
        f(i,m)sc(k),b.pb(k);
        if(rec(m-1))printf("Yes\n");
        else printf("No\n");
    }

    return 0;
}
