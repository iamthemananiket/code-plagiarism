# include <cstdio>
# include <cmath>
# include <cstring>
# include <string>
# include <vector>
# include <queue>
# include <map>
# include <algorithm>
# include <iostream>

using namespace std;

int n, m;

int main ()
{
	int t, i;
	scanf ("%d", &t);
	while (t --)
	{
		scanf ("%d%d", &n, &m);
		if (n != m)
			printf ("-1 -1\n");
		else
		{
			for (i = 1; i < n; i ++)
				printf ("%d %d\n", i, i + 1);
			printf ("%d 1\n", n);
		}
	}
	return 0;
}
