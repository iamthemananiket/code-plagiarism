#include<bits/stdc++.h>
using namespace std;

int main()
{
//	freopen("in.txt","r",stdin);
//	freopen("out.txt","w",stdout);
	
	int t;
	cin>>t;
	int n;
	while(t--)
	{
		cin>>n;
		if(n==1)
		  cout<<2<<endl;
		else 
		 {
		 	if((n&(n+1))==0)
		 	   cout<<n/2<<endl;
		 	else 
		 	cout<<"-1\n";
		 }
		
	}
}