using namespace std;

int main() {
    ios::sync_with_stdio(0);
    int a, b, c;
    cin >> a >> b >> c;
    c -= a;
    if ((c / b) % 2 == 1)
        cout << "Unlucky Chef\n";
    else
        cout << "Lucky Chef\n";
    return 0;
}