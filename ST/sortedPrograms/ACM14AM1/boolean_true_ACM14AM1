#include<bits/stdc++.h>
using namespace std;
int arr[2000009];
int main()
{
    int test;
    scanf("%d",&test);
    while(test--)
    {
        int n,ans=0,p;
        scanf("%d%d",&n,&p);
        for(int i=0;i<n;i++)
        {
            scanf("%d",&arr[i]);
            if(arr[i]>=p)
                ans++;
        }
        printf("%d\n",ans);
    }
}
