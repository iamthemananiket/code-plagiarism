#include <iostream>
#include <algorithm>
using namespace std;

int main()
{
        ios_base::sync_with_stdio(0);
        cin.tie(0);

        int  t, n, m, x ;

        for(  cin >> t ;  t-- ;  )
        {
                cin >> n >> m ;
                x = n / __gcd(n,m) ;
                if( x == n )  cout << "Yes" << endl ;
                else   cout << "No " << x << endl ;
        }

        return 0;
} 