using namespace std;

int main() {
	int T;
	scanf(" %d",&T);
	for(int t =0; t < T; t++) {
		int h1,m1,h2,m2,dist;
		scanf(" %d:%d %d:%d %d",&h1,&m1,&h2,&m2,&dist);
		printf("%.1f ",(h1-h2)*60.0+m1-m2+dist);
		if(2*dist <= (h1-h2)*60+m1-m2) printf("%.1f\n",(h1-h2)*60.0+m1-m2);
		else printf("%.1f\n",(h1-h2)*60.0+m1-m2+(2*dist-(h1-h2)*60-m1+m2)/2.0);}

	return 0;}

