int main()
{//freopen("out2.txt","w",stdout);
	sieve();
//	eulertot();
	int t,n,d;
	cin>>t;
	while(t--)
	{
		cin>>n>>d;
		int lmt=n/d;
		long long sum=SUM[lmt/STEP];
		for(int i= lmt - lmt%STEP + 1;i<=lmt;i++)
		 {
		 	sum+=phi(i);
		 //	cout<<i<<" "<<phi(i)<<endl;
		 }	
		cout<<sum<<endl;
		
	}
}