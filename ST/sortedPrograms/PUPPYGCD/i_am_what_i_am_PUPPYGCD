void sieve()
{
	for(int i=3;i<30005;i+=2)
	  primeTest[i]=true;
	primes.push_back(2);

	for(int i=3;i<30005;i+=2)
	 {
	 	if(!primeTest[i])
	 	  continue;
	 	primes.push_back(i);
	 	for(int j=i*i;j<30005;j+=2*i)
		  {primeTest[j]=false;
		  }
	 }
	
}

int phi(int n)
{int ans=n;
 for(int i=0;i<primes.size() && primes[i]*primes[i]<=n;i++) 
 {
 	if(n%primes[i]==0)
 	 {
 	 	ans-=ans/primes[i];
 	 	while(n%primes[i]==0)
 	 	  n/=primes[i];
	  }
 }
 if(n>1)
   ans-=ans/n;
return ans;
}
