int main() {
	vector<bool> a(100000, true);
	vector<int> p;
	a[0] = a[1] = false;
	for (int i = 2; i < 100000; i++)
		if (a[i]) {
			p.push_back(i);
			for (int j = i * 2; j < 100000; j += i)
				a[j] = false;
		}
	int T; cin >> T;
	while (T--) {
		int N, D; cin >> N >> D;
		int n = N / D;
		int k = min(n / 200000, 2499);
		ll sum = umekomi[k];
		for (int i = k * 200000 + 1; i <= n; i++) {
			int _i = i, e = _i;
			for (int k = 0; p[k] * p[k] <= _i; k++)
				if (_i % p[k] == 0) {
					e -= e / p[k];
					for (; _i % p[k] == 0; _i /= p[k]);
				}
			if (_i > 1) e -= e / _i;
			sum += e;
		}
		cout << sum << endl;
	}
	/*ll sum = 0;
