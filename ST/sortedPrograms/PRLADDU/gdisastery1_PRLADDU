#include <cstdio>
#include <cstring>
#include <cmath>
#include <iostream>
#include <vector>
#include <set>
#include <map>
#include <queue>
#include <stack>
#include <string>
#include <algorithm>
using namespace std;
#define SZ(x) ( (int) (x).size() )
#define dbg(x) cerr << #x << " = " << x << endl;
#define mp make_pair
#define pb push_back
#define fi first
#define se second
typedef long long ll;
typedef pair<int, int> pii;
// const int INF = 1e9;

int T, N;

int main(){
  scanf("%d", &T);
  while(T--){
    scanf("%d", &N);
    ll r = 0;
    int x, a = 0;
    for(int i = 0; i < N; i++){
      scanf("%d", &x);
      a += x;
      r += abs(a);
    }
    printf("%lld\n", r);
  }
  return 0;
}
