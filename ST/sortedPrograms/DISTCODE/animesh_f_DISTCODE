// LTIME

#include <bits/stdc++.h>
using namespace std;

int T;
string S;
map < string , int > M;

int main(){
	
	cin.tie(0) , ios::sync_with_stdio(false);
	cin >> T;
	
	while(T--){
		
		cin >> S;
		
		M.clear();
		string temp = "";
		
		for(int i = 0 ; i < S.size() - 1 ; i++){
			temp = (S[i]);
			temp += (S[i + 1]);
			M[temp] = 1;
			temp = "";
		}

		cout << M.size() << '\n';
	}

}
