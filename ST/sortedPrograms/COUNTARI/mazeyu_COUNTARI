#include<bits/stdc++.h>
#define N 100010
using namespace std;
unsigned int n,i,f1[N],f2[N],a[N],*p1,*p2,*p3,x1;
long long ans;
int main()
{
	scanf("%d",&n);
	for(i=1;i<=n;i++)
	{
		scanf("%d",&a[i]);
		f2[30000-a[i]]++;
	}
	for(i=1;i<=n;i++)
	{
		f2[30000-a[i]]--;
		if(2*a[i]<=30000)x1=0;
		else x1=2*a[i]-30000;
		p1=f1+x1;
		p2=f2+30000-2*a[i]+x1;
		p3=f1+2*a[i]-x1;
		for(;p1<=p3;p1+=20,p2+=20)
			ans+=(*p1)*(*p2)+(*(p1+1))*(*(p2+1))+(*(p1+2))*(*(p2+2))+(*(p1+3))*(*(p2+3))+(*(p1+4))*(*(p2+4))+(*(p1+5))*(*(p2+5))+(*(p1+6))*(*(p2+6))+(*(p1+7))*(*(p2+7))+(*(p1+8))*(*(p2+8))+(*(p1+9))*(*(p2+9))
			+(*(p1+10))*(*(p2+10))+(*(p1+11))*(*(p2+11))+(*(p1+12))*(*(p2+12))+(*(p1+13))*(*(p2+13))+(*(p1+14))*(*(p2+14))+(*(p1+15))*(*(p2+15))+(*(p1+16))*(*(p2+16))+(*(p1+17))*(*(p2+17))+(*(p1+18))*(*(p2+18))+(*(p1+19))*(*(p2+19));
		f1[a[i]]++;
	}
	printf("%lld\n",ans);
}