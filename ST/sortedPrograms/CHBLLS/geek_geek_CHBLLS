#define sd(x) scanf("%d",&x)
#define sd2(x,y) scanf("%d%d",&x,&y)
#define sd3(x,y,z) scanf("%d%d%d",&x,&y,&z)
#define fi first
#define se second
#define pb(x) push_back(x)
#define mp(x,y) make_pair(x,y)
#define r0 return 0
#define mod 1000000007
using namespace std;
int prog()
{
     fflush(stdout);
    int i,j,k,x,y,z;
    cout<<"1\n3 1 2 2\n3 3 4 4"<<endl;
    fflush(stdout);
    cin>>x;
    if(x==0)
    {
        cout<<2<<endl<<5<<endl;
        fflush(stdout);
    }
   else if(x==1)
   {
       cout<<2<<endl<<1<<endl;
        fflush(stdout);
   }
   else if(x==-1)
   {
       cout<<2<<endl<<3<<endl;
        fflush(stdout);
   }   else if(x==2)
   {
       cout<<2<<endl<<2<<endl;
        fflush(stdout);
   }else if(x==-2)
   {
       cout<<2<<endl<<4<<endl;
        fflush(stdout);
   }
    r0;
}
int main()
{
        prog();
    r0;
}
