#include <bits/stdc++.h>

using namespace std;

int main()
{
    int T;
    scanf("%d", &T);
    while(T--)
    {
        int N, a, b;
        scanf("%d", &N);
        int ans=0;
        for(int i=1; i<=N; i++)
            ans^=i, scanf("%d%d", &a, &b);
        printf("%d\n", ans);
    }
    return 0;
}
