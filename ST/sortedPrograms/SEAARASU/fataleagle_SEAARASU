#include <bits/stdc++.h>

using namespace std;

vector<int> ppl;

int main()
{
    int T;
    scanf("%d", &T);
    while(T--)
    {
        int N;
        scanf("%d", &N);
        int g=0;
        for(int i=0; i<N; i++)
        {
            int a;
            scanf("%d", &a);
            g=__gcd(g, a);
        }
        printf("%lld\n", 1LL*g*N);
    }
    return 0;
}
