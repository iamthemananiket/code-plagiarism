#include <cstdio>
#include <cmath>
#include <iostream>
#include <set>
#include <algorithm>
#include <vector>
#include <map>
#include <cassert>
#include <string>
#include <cstring>
#include <queue>

using namespace std;

#define rep(i,a,b) for(int i = a; i < b; i++)
#define S(x) scanf("%d",&x)
#define S2(x,y) scanf("%d%d",&x,&y)
#define P(x) printf("%d\n",x)
#define all(v) v.begin(),v.end()
#define FF first
#define SS second

typedef long long int LL;
typedef pair<int, int > pii;
typedef vector<int > vi;

int main() {
  int t;
  S(t);
  while(t--) {
    int n;
    S(n);
    int g;
    rep(i,0,n) {
      int x;
      S(x);
      if(!i) g = x;
      g = __gcd(g,x);
    }
    printf("%lld\n",g*1LL*n);
  }
  return 0;
}