#include<bits/stdc++.h>
using namespace std;

int main()
{
	int t;
	cin>>t;
	while(t--)
	{
		
		int n,m;
		cin>>n;
		
		vector<int> a(n);
		for(int i=0;i<n;i++)
		 {
		 	cin>>a[i];
		 }
		cin>>m;
		vector<int> b(m);
		for(int i=0;i<m;i++)
		 {
		 	cin>>b[i];
		 }
		 int p=0;
		 int i=0;
		 while(i<n && p<m)
		 {
		 	if(a[i]==b[p])
		 	 {
		 	 	p++;
		 	 	
			  }
			i++;
		 }
		if(p==m)
		  cout<<"Yes"<<endl;
		 else
		   cout<<"No"<<endl;		
	}
	
}