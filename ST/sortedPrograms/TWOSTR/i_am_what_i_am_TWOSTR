#include<bits/stdc++.h>
using namespace std;

int main()
{
//	freopen("in.txt","r",stdin);
//	freopen("out.txt","w",stdout);
	
	int t;
	cin>>t;
	string a,b;
	while(t--)
	{
		cin>>a>>b;
		int i;
		for(i=0;i<a.size();i++)
		{
			if(a[i]=='?' || b[i]=='?')
			 continue;
			if(a[i]!=b[i])
			 break;
		}
		if(i==a.size())
			puts("Yes");
		else
		  puts("No");
 	}
}