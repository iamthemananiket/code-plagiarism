#include<bits/stdc++.h>
 using namespace std;
  int main()
  {

    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);

   // freopen("input.txt","r",stdin);
   // freopen("output.txt","w",stdout);

        int t;
     cin>>t;
      while(t--)
      {
       string x,y;
        cin>>x;
        cin>>y;
        char tt = 1;
         for(int i=0;i<x.size();++i)
          if(x[i]!='?' && x[i]!=y[i] && y[i]!='?')
          {
            tt=0;
            break;
          }

          if(tt)cout<<"Yes";else cout<<"No"; cout<<"\n";

      }

    return 0;
  }
