#include <bits/stdc++.h>

using namespace std;

namespace stuff {

void solve(int test_num) {
  int N, a[111];
  cin >> N;
  for (int i = 0; i < N; i++)
    cin >> a[i];
  sort(a, a + N);
  cout << "Case " << test_num << ": " << (a[N - 1] + a[N - 2] + a[N - 3]) << endl;
}

void solve() {
#ifdef AZN
  freopen("input.txt", "r", stdin);
  freopen("output.txt", "w", stdout);
//freopen("azn.txt", "w", stderr);
  double start_t = clock();
#endif
  ios::sync_with_stdio(false);
  cin.tie(NULL);
  int T = 1;
  cin >> T;
  for (int t = 1; t <= T; t++)
    solve(t);
#ifdef AZN
  pln("Took:", (clock() - start_t) / CLOCKS_PER_SEC);
#endif
}
}

int main() {
  stuff::solve();
  return 0;
}

