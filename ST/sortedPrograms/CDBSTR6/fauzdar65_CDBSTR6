#include <bits/stdc++.h>
#define ll long long
#define pb push_back
#define mp make_pair
#define mod 1000000007
#define gc getchar_unlocked
#define pp pair<int,int>
using namespace std;


int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t,a,n;

cin>>t;

while(t--)
{
  
    cin>>n;
    int ma=-1,sma=-1;
    for(int i=0;i<n;i++){ cin>>a;if(a>ma){sma=ma; ma=a;} else if(a>sma)sma=a;}
    cout<<(ma&sma)<<"\n";
}

	return 0; 
}