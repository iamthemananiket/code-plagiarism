#include<bits/stdc++.h>
using namespace std;
char s[10000+10];
int main()
{
	int t;
	scanf("%d",&t);
	while (t--)
	{
		scanf("%s",s);
		int i,j,a=0,b=0,l=strlen(s);
		for (i=0;s[i]!='\0';i++)
		{
			if (s[i]=='W')
				break;
		}
		a=i;
		b=l-1-a;
		if (a==b)
		{
			printf("%s\n","Chef");
		}
		else
		{
			printf("Aleksa\n");
		}
	}
	return 0;
}

		