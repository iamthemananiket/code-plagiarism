typedef long long ll;
const int MOD = int(1e9) + 7;

bool check(const vector<int>& a, const int N) {
  vector<vector<int>> cnt(N, vector<int>(N, 0));
  if ((int) a.size() != N * N) {
    return false;
  }
  for (int i = 0; i < N * N; ++i) {
    int sum = 0;
    for (int k = 0; k < N * N; ++k) {
      sum += a[(i % N) * N + k % N] * a[(i / N) * N + k % N];
    }
    if (i % N == i / N && sum != N * N) {
      return false;
    }
    if (i % N != i / N && sum != 0) {
      return false;
    }
  }
  return true;
}

void solve(int test_num) {
  int N;
  cin >> N;
  if (N == 12) {
    cout << "YES\n";
    for (int i = 0; i < N * N; ++i) {
      cout << H12[i] << ' ';
    }
    cout << endl;
    return;
  }
  if (N == 20) {
    cout << "YES\n";
    for (int i = 0; i < N * N; ++i) {
      cout << H20[i] << ' ';
    }
    cout << endl;
    return;
  }
  if (N == 24) {
    cout << "YES\n";
    vector<int> res(N * N);
    for (int i = 0; i < N * N; ++i) {
      res[i] = H24[i];
      cout << H24[i] << ' ';
    }
#ifdef AZN
    dbgln(N);
    dbgln(check(res, N));
#endif
    cout << endl;
    return;
  }
  if (N == 28) {
    cout << "YES\n";
    vector<int> res(N * N);
    for (int i = 0; i < N * N; ++i) {
      res[i] = H28[i];
      cout << H28[i] << ' ';
    }
#ifdef AZN
    dbgln(N);
    dbgln(check(res, N));
#endif
    cout << endl;
    return;
  }
  if (N > 2 && N % 4) {
    cout << "NO\n";
    return;
  }
  vector<bool> vis(1 << N, false);
  int at = 0;
  vector<int> vals;
  for (int i = 0; !vis[at]; ++i) {
    vals.push_back(at);
    vis[at] = true;
    for (int nx = 0; nx < (1 << N); ++nx) {
      if (vis[nx]) {
        continue;
      }
      bool ok = true;
      for (const int& x : vals) {
        if (2 * __builtin_popcount(x ^ nx) != N) {
          ok = false;
          break;
        }
      }
      if (ok) {
        at = nx;
        break;
      }
    }
  }
  sort(vals.begin(), vals.end());
  vector<int> res;
  for (const int& x : vals) {
    for (int b = N - 1; b >= 0; --b) {
      if (1 << b & x) {
        res.emplace_back(1);
      } else {
        res.emplace_back(-1);
      }
    }
  }
  //assert((int )res.size() == N * N);
#ifdef AZN
  dbgln(N);
  dbgln(check(res, N));
//  for (const int& x : vals) {
//    for (int b = N - 1; b >= 0; --b) {
//      cerr << (x >> b & 1);
//    }
//    cerr << endl;
//  }
#endif
  cout << "YES\n";
  for (const int& x : res) {
    cout << x << ' ';
  }
  cout << endl;
}

void solve() {
#ifdef AZN
//make_case();
  double start_t = clock();
  freopen("input.txt", "r", stdin);
  freopen("output.txt", "w", stdout);
//freopen("azn.txt", "w", stderr);
#endif
  ios::sync_with_stdio(false);
  cin.tie(NULL);
  int T = 1;
//scanf("%d", &T);
  cin >> T;
  for (int t = 1; t <= T; t++)
    solve(t);
#ifdef AZN
  cerr << fixed << setprecision(3) << "Took: " << ((clock() - start_t) / CLOCKS_PER_SEC);
#endif
}
}

int main() {
  stuff::solve();
  return 0;
}
