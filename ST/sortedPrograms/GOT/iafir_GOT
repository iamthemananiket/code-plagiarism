#include <iostream>
#include <cstdio>
     
using namespace std;
     
int t,n;

int main()
{
    scanf("%d",&t);
    for(int i=0 ; i<t; i++)
    {
    	scanf("%d",&n);
    	if(n % 2)
    		printf("Lannisters always pays their debts\n");
		else
			printf("Valar Morghulis\n");
    }
    return 0;
}

