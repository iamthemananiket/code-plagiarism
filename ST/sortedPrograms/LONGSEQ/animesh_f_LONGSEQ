#include "bits/stdc++.h"
using namespace std;

int main(){
    ios :: sync_with_stdio(false);
    int t; cin >> t;
    while(t--){
        string s; cin >> s;
        int o = 0, z = 0;
        for(int i = 0; i < (int) s.size(); i++){
            if(s[i] == '0') z++;
            else o++;
        }
        if(o == 1 || z == 1) cout << "Yes\n";
        else cout << "No\n";
    }
}
