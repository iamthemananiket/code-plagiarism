#include <bits/stdc++.h>
 
using namespace std;

typedef long long ll;

int main() {
  ios::sync_with_stdio(false);
  cin.tie(nullptr);
  int T;
  cin >> T;
  while (T-- > 0){
    string s;
    cin>>s;
    map<char,int> cnt;
    for(char c:s)
      ++cnt[c];
    if(cnt['0']==1||cnt['1']==1)
      cout<<"Yes\n";
    else
      cout<<"No\n";
  }
  
  return 0;
}