#include<bits/stdc++.h>
using namespace std;

int main()
{
	int t;
	cin>>t;
	while(t--) {
		
		int n,k,x;
		cin>>n>>k;
		for(int i=0;i<n;i++)
		 {
		 	for(int j=0;j<n;j++)
		 	 {
		 	 	cin>>x;
			  }
		 }
		for(int i=0;i<k;i++)
		 {
		 	cin>>x;
		 }
		for(int i=0;i<n;i++)
		 {
		 	for(int j=0;j<n;j++)
		 	 {
		 	 	int pos=i*n+j+1;
		 	 	if(pos<=k)
		 	 	  cout<<pos<<" ";
		 	 	else
		 	 	  cout<<"0 ";
			  }
			  cout<<endl;
		 }
	}
}