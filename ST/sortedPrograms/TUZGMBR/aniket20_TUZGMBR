#include<bits/stdc++.h>
using namespace std;

// Numeric Constants
#define N 1000000007
#define maxs 200005
#define mins 1005
#define eps 0.000000000001
#define imax 2000000200
#define llmax 1000000002000000000ll
#define pi 3.141592653589793

// Others
#define ll long long
#define pb push_back
#define gc getchar_unlocked
#define iosbase ios_base::sync_with_stdio(false)
#define pii pair<int,int>
#define pll pair<ll,ll>
#define ppi pair<pair<int,int>,int>
#define ppl pair<pll,ll>
#define vi vector<int>
#define sc scanf
#define pr printf
#define lld I64d
#define F first
#define S second
#define siter set<int>::iterator
#define p_pq priority_queue
#define ub upper_bound
#define lb lower_bound

int n,m;

int main()
{
	int t,i,j;
	sc("%d",&t);
	while(t--){
		sc("%d %d",&n,&m);
		int temp=(n-1)/4;
		n-=temp*4;
		if(n==1){
			int t1=(m-1)/3;
			m-=t1*3;
			if(m==1){
				pr("Vanya\n");
			}
			else{
				pr("Tuzik\n");
			}
		}
		else if(n==2){
			int t1=(m-1)/3;
			m-=t1*3;
			if(m==2){
				pr("Vanya\n");
			}
			else{
				pr("Tuzik\n");
			}
		}
		else if(n==3){
			int t1=(m-1)/3;
			m-=t1*3;
			if(m==3){
				pr("Vanya\n");
			}
			else{
				pr("Tuzik\n");
			}
		}
		else{
			pr("Tuzik\n");
		}
	}
	return 0;
}