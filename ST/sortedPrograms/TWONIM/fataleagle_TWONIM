#include <bits/stdc++.h>

using namespace std;

int main()
{
    int T;
    scanf("%d", &T);
    while(T--)
    {
        int N;
        scanf("%d", &N);
        int x=0;
        for(int i=0; i<N; i++)
        {
            int a;
            scanf("%d", &a);
            x^=a;
        }
        if(x==0 || N%2==0)
            printf("First\n");
        else
            printf("Second\n");
    }
    return 0;
}
