#include<bits/stdc++.h>
using namespace std;
int main()
{
	long long int t,N,A,B,C,ans,temp;
	scanf("%lld",&t);
	while(t--)
	{
		scanf("%lld%lld%lld%lld",&N,&A,&B,&C);
		ans=(N+3)*(N+2)*(N+1)/6;
		if(N>=A+1)
		{
			temp=3+N-A-1;
			ans-=temp*(temp-1)*(temp-2)/6;
		}
		if(N>=B+1)
		{
			temp=3+N-B-1;
			ans-=temp*(temp-1)*(temp-2)/6;
		}
		if(N>=C+1)
		{
			temp=3+N-C-1;
			ans-=temp*(temp-1)*(temp-2)/6;
		}
		if(N>=A+1+B+1)
		{
			temp=3+N-A-1-B-1;
			ans+=temp*(temp-1)*(temp-2)/6;
		}
		if(N>=A+1+C+1)
		{
			temp=3+N-A-1-C-1;
			ans+=temp*(temp-1)*(temp-2)/6;
		}
		if(N>=B+1+C+1)
		{
			temp=3+N-B-1-C-1;
			ans+=temp*(temp-1)*(temp-2)/6;
		}
		if(N>=A+1+B+1+C+1)
		{
			temp=3+N-A-1-B-1-C-1;
			ans-=temp*(temp-1)*(temp-2)/6;
		}
		printf("%lld\n",ans);
	}
	return 0;
}