#include <bits/stdc++.h>

using namespace std;

typedef long long ll;
int ra,k;
void solve(){
    int y=int(sqrt(2*ra-1));
    ll rem=10000LL*(2*ra-1-1LL*y*y);
    if (200LL*k*ra-k*k>=rem) printf("%d %d\n",ra-1,y);
    else printf("-1\n");
}
int main(){
    int test;
    cin>>test;
    while (test--){
        cin>>ra>>k;
        solve();
    }
}
